<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.intrusoft.islibrarydemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".HomeActivity"
            android:label="@string/title_activity_home"
            android:theme="@style/AppTheme" />

        <activity
            android:name=".SquintActivity"
            android:label=""
            android:parentActivityName=".HomeActivity" />

        <activity
            android:name=".FlareActivity"
            android:label=""
            android:parentActivityName=".HomeActivity" />

        <activity
            android:name=".FunkyHeaderActivity"
            android:label=""
            android:parentActivityName=".HomeActivity" />

        <activity
            android:name=".SectionedRecyclerViewActivity"
            android:label=""
            android:parentActivityName=".HomeActivity" />

        <activity
            android:name=".BlazeZoomActivity"
            android:label=""
            android:parentActivityName=".HomeActivity" />

        <activity
            android:name=".BlazeMotionActivity"
            android:label=""
            android:parentActivityName=".BlazeZoomActivity" />

        <activity
            android:name=".ScatterPieChartActivity"
            android:label=""
            android:parentActivityName=".HomeActivity" />

        <activity
            android:name=".PieChartActivity"
            android:label=""
            android:parentActivityName=".ScatterPieChartActivity" />

        <activity
            android:name=".SimpleChartActivity"
            android:label=""
            android:parentActivityName=".ScatterPieChartActivity" />

    </application>

</manifest>